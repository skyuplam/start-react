{
  "name": "start-react",
  "version": "0.1.0",
  "description": "A boilerplate for starting react project.",
  "main": "index.js",
  "repository": "git@github.com:skyuplam/start-react.git",
  "author": "Terrence Lam <skyuplam@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.8.1",
    "koa": "^2.0.0",
    "koa-compress": "next",
    "koa-mount": "next",
    "koa-static": "next",
    "offline-plugin": "^4.5.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "next",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.10",
    "serialize-javascript": "^1.3.0"
  },
  "scripts": {
    "build": "yarn run clean && yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production babel-node ./internal/scripts/build.js",
    "build:server": "cross-env NODE_ENV=production babel src -d dist",
    "clean": "rimraf dist build",
    "lint": "eslint src",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server",
    "serve": "cross-env NODE_ENV=production node dist/server"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "beta",
    "happypack": "^3.0.2",
    "koa-webpack-middleware": "git://github.com/skyuplam/koa-webpack-middleware.git#local-git",
    "react-hot-loader": "next",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "beta",
    "webpack-node-externals": "^1.5.4"
  }
}
